<h1>Comedians</h1>
<% @comedians.each do |comedian| %>
  <div id="comedian-<%=comedian.id%>">
    <p><%= comedian.name %></p>
    <p><%= comedian.age %></p>
    <p><%= comedian.city %></p>
    <% comedian.specials.each do |special| %>
      <p> <%= special.name %></p>
    <% end %>
  </div>
<% end %>
